group("tests") {
  testonly = true

  deps = [
    ":modular_tests.json",
    "common",
    "component_context",
    "context_link",
    "last_focus_time",
    "link_data",
    "link_update",
    "parent_child",
    "queue_persistence",
    "remote",
    "story_importance",
    "suggestion",
    "triggers",
    "user_shell",
  ]
}

copy("modular_tests.json") {
  sources = [
    "modular_tests.json",
  ]
  outputs = [
    "$root_out_dir/modular_tests.json",
  ]
}

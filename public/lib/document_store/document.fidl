module document_store;

struct Statement {
  string docid;
  string property;
  Value? value;
};

struct Document {
  string docid;

  // Map from RDF key to value.
  map<string, Value?>? properties;
};

union Value {
  string string_value;
  int64 int_value;
  double float_value;
  array<uint8> binary;
  string iri;
  // Represents an empty value.
  bool empty;
};
